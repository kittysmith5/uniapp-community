<block wx:for="{{leftList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="feed-one data-v-57280228"><navigator open-type="navigate" url="{{'/subpages/feedinfo/feedinfo?id='+item.id}}" class="data-v-57280228"><image class="feed-img data-v-57280228" src="{{item.cover}}" mode="widthFix" lazy-load="{{true}}"></image><block wx:if="{{!!item.feed_content}}"><view class="u-line-2 feed-title data-v-57280228">{{item.feed_content+''}}</view></block><view class="feed-info data-v-57280228"><view class="iview data-v-57280228"><image class="avatar data-v-57280228" src="{{item.avatar}}"></image><text class="name u-line-1 data-v-57280228">{{item.name}}</text></view><view class="iview data-v-57280228"><view data-event-opts="{{[['tap',[['clickLove',['$0'],[[['leftList','',index]]]]]]]}}" class="ilike data-v-57280228" catchtap="__e"><block wx:if="{{item.has_like}}"><image class="micon data-v-57280228" src="/static/lover.png"></image></block><block wx:else><image class="micon data-v-57280228" src="/static/love.png"></image></block><block wx:if="{{item.like_count>0}}"><text class="love-count data-v-57280228">{{''+item.like_count+''}}</text></block></view></view></view></navigator></view></block>